{
  "redirects": [
    {
      "source": "/chess",
      "destination": "/chessboard",
      "permanent": false
    },
    {
      "source": "/register",
      "destination": "/register",
      "permanent": false
    },
    {
      "source": "/login",
      "destination": "/login",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/build/index.html"
    }
  ],
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
   
    }
  ]
}
