

{
  "version": 2,
  "builds": [
    {
      "src": "frontend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {"source": "/(.*)", "destination": "/"}
  ]
}
