{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
 
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.js" },
    { "src": "/register", "dest": "frontend/app.js" },
    { "src": "/login", "dest": "frontend/app.js" },
    { "src": "/chess", "dest": "frontend/app.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
